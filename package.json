    "mustache": "^2.3.0",
    "mustache-express": "^1.2.4"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "coveralls": "^2.11.15",
    "istanbul": "^0.4.5",
    "jshint": "^2.9.3",
    "mocha": "^3.2.0",
    "supertest": "^2.0.0"
